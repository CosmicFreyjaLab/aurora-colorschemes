      "Actions.Yellow": "#f5d67f",
      "Actions.Purple": "#9966ff",
      "Objects.Blue": "#3b9eff",
      "Objects.Green": "#5df0b0",
      "Objects.Grey": "#607090",
      "Objects.Pink": "#e571b5",
